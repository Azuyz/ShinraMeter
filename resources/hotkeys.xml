<?xml version="1.0" encoding="UTF-8"?>
<hotkeys>
	<paste>
		<ctrl>false</ctrl>
		<shift>false</shift>
		<window>false</window>
		<key>Home</key>
	</paste>
	<copys>

		<copy>
			<ctrl>true</ctrl>
			<shift>false</shift>
			<window>false</window>
			<key>End</key>
			<string>
				<header>Dégats reçu:\</header>
				<content>{name}: {damage_received}\</content>
				<footer></footer>
				<order_by>damage_received</order_by>
				<order>descending</order>
			</string>
		</copy>
		<copy>
			<ctrl>false</ctrl>
			<shift>true</shift>
			<window>false</window>
			<key>End</key>
			<string>
				<header>{encounter} {timer} => % degats | % crit | dps:\</header>
				<content>{name}: {damage_percentage} {crit_rate} {dps}\</content>
				<footer></footer>
				<order_by>damage_percentage</order_by>
				<order>descending</order>
			</string>
		</copy>
	</copys>
	<reset>
		<shift>false</shift>
		<ctrl>false</ctrl>
		<alt>false</alt>
		<window>false</window>
		<key>Delete</key>
	</reset>
</hotkeys>